<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">

	<!-- id에 해당되는 1건 데이터 조회 -->
	<select id="memberListById" parameterType="String" resultType="vo.MemberVo">
		select * from member3 where m_id=#{m_id}
	</select>
	
</mapper>












